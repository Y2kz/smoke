[AES-256] (1)
{
	A Timestamp (1)
	Key Type (2)
	Public Key (3)
	Public Key Signature (4)
	Signature Public Key (5)
	Signature Public Key Signature (6)
}

[SHA-512 HMAC] (2)
{
	[AES-256] (1)
}

/*
** The destination is created via the recipient's SipHash identity.
*/

[Destination SHA-512 HMAC] (3)
{
	[AES-256] || [SHA-512 HMAC] (1)
}
