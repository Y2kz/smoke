[AES-256] (1)
{
	Key Type (1)
	Public Key (2)
	Public Key Signature (3)
	Signature Public Key (4)
	Signature Public Key Signature (5)
	Timestamp (6)
}

[SHA-512 HMAC] (2)
{
	[AES-256] (1)
}

/*
** The destination is created via the recipient's SipHash identity.
*/

[Destination SHA-512 HMAC] (3)
{
	[AES-256] || [SHA-512 HMAC] (1)
}
