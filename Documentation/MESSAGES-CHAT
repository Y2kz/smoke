[PK] (1)
{
	AES-256 Key
	SHA-512 Key
}

[AES-256] (2)
{
	Message (1)
	Sequence (2)
	Timestamp (3)
	Sender's Public Encryption Key SHA-512 Digest (4)
	[PK Signature] (5)
	{
		[PK] || [AES-256] (1 ... 4)
	}
}

[SHA-512 HMAC] (3)
{
	[PK] || [AES-256]
}

[Destination SipHash HMAC] (4)
{
	[PK] || [AES-256] || [SHA-512 HMAC] (1)
}
